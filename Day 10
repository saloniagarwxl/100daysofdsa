//Problem: Read a string and check if it is a palindrome using two-pointer comparison.

#include <stdio.h>
#include <string.h>

int main() {
    char s[100];
    int i;

    scanf("%s", s);

    for(i = 0; i < strlen(s)/2; i++) {
        if(s[i] != s[strlen(s)-1-i]) {
            printf("NO");
            return 0;
        }
    }

    printf("YES");
    return 0;
}
